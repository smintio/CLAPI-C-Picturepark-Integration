{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }  
  },
  "AllowedHosts": "*",
  "SmintIo": {
    "App": {
      "TenantId": "<SMINTIO-TENANT-ID>",
      "ChannelId": -1,
      "ImportLanguages": [ "en", "de" ]
    },
    "Auth": {
      "ClientId": "<SMINTIO-OAUTH-CLIENT-ID>",
      "ClientSecret": "<SMINTIO-OAUTH-CLIENT-SECRET>",
      "RedirectUri": "http://127.0.0.1:7587"
    }
  },
  "Picturepark": {
    "App": {
      "ApiBaseUrl": "https://api.<ASK-PICTUREPARK>-picturepark.com",
      "CustomerAlias": "<PICTUREPARK-CUSTOMER-ALIAS>",
      "PictureparkFileTypes": [ "FileMetadata", "AudioMetadata", "DocumentMetadata", "ImageMetadata" ],
      "ContentLayerNames": {
        "x-default": "Smint.Io",
        "en": "Smint.Io",
        "de": "Smint.Io"
      },
      "LicenseLayerNames": {
        "x-default": "Smint.Io License Info",
        "en": "Smint.Io License Info",
        "de": "Smint.Io License Info"
      }
    },
    "Auth": {
      "AccessToken": "<PICTUREPARK-AUTH-TOKEN>"
    }
  }
}